{
  "name": "kujira-api",
  "version": "0.3.0",
  "description": "Your spending habits made clear.",
  "author": "yabakune",
  "engines": {
    "node": ">=19.4.0"
  },
  "private": true,
  "main": "server.ts",
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules",
      "src/**/*.test.ts",
      "src/**/*.spec.ts"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "npx tsc",
    "start": "node dist/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "kujira",
    "money",
    "self-help"
  ],
  "license": "ISC",
  "dependencies": {
    "@netlify/functions": "^2.1.0",
    "@prisma/client": "^4.16.2",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "nodemailer": "^6.9.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/nodemailer": "^6.4.8",
    "nodemon": "^2.0.22",
    "prisma": "^4.16.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
